  },
  {
   "cell_type": "code",
   "execution_count": 18,
   "id": "fad3d6c9",
   "metadata": {},
   "outputs": [],
   "source": [
    "non_students = Credit[Credit['Student'] == 'No']"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 19,
   "id": "b818baa7",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "np.float64(47.29205)"
      ]
     },
     "execution_count": 19,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "students['Income'].mean()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 20,
   "id": "acdb144a",
   "metadata": {},
   "outputs": [
    {
     "data": {
